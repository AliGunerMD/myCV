	journal = {Lancet Glob Health},
	journal-full = {The Lancet Global health},
	mesh = {Humans; Female; Middle Aged; Male; Prospective Studies; Stomach Neoplasms; Neoplasm Recurrence, Local; Malnutrition; Colorectal Neoplasms},
	month = {Mar},
	number = {3},
	pages = {e341-e349},
	pmid = {36796981},
	pst = {ppublish},
	rating = {3},
	read = {0},
	title = {Impact of malnutrition on early outcomes after cancer surgery: an international, multicentre, prospective cohort study},
	volume = {11},
