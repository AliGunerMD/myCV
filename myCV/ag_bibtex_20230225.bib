	journal = {Lancet Glob Health},
	journal-full = {The Lancet. Global health},
	mesh = {Hospitals; Humans; Income; Neoplasms; Postoperative Period; Poverty; Prospective Studies},
	month = {Jul},
	number = {7},
	pages = {e1003-e1011},
	pmc = {PMC9210173},
	pmid = {35623378},
	pst = {ppublish},
	title = {Effects of hospital facilities on patient outcomes after cancer surgery: an international, prospective, observational study},
	volume = {10},
	year = {2022},
