	journal = {Support Care Cancer},
	journal-full = {Supportive care in cancer : official journal of the Multinational Association of Supportive Care in Cancer},
	mesh = {Aged; Antidiarrheals; Diarrhea; Female; Glutamine; Humans; Male; Middle Aged; Neoplasms; Turkey},
	month = {Apr},
	number = {4},
